    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "@upstatement/eslint-config": "^0.3.1",
    "@upstatement/prettier-config": "^0.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
